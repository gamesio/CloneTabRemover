	"name": "Clone Tab Remover",
	"version": "1.01",
	"description": "Help you to eliminate redundant pages.",
	"permissions": ["history", "tabs"],
	"background": {
		"scripts": ["Remover.js"],
		"persistent": false
	},
	"browser_action":{
